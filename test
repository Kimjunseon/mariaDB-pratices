upp_dept_eng_nm varchar(400),
company_cd varchar(100),
company_nm varchar(100),
dept_mngr_user_no varchar(100),
dept_mngr_user_nm varchar(400),
location_cd varchar(100),
location_nm varchar(400),
location_eng_nm varchar(400),
dept_grade_cd varchar(100),
dept_ord varchar(100),
create_y varchar(20),
delete_y varchar(20),
chg_type varchar(20),
use_flg varchar(1),
chg_dttm date,
PRIMARY KEY(dept_cd));

CREATE TABLE IF NOT EXISTS sys_email_log(
email_log_id varchar(20) NOT NULL,
send_dt date,
