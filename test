CREATE TABLE IF NOT EXISTS sys_com_cd(
com_cd_id varchar(20) not null unique key,
com_cd varchar(20) not null,
supi_com_cd_id varchar(20),
supi_com_cd varchar(20),
lang_cd varchar(2) not null,
code_nm varchar(20) not null,
desc_txt varchar(100),
manage_yn char(1),
sort_no int,
use_yn char(1),
st_dttm date,
end_dttm date);

 CREATE TABLE IF NOT EXISTS el_user_info(
 user_id varchar(20) UNIQUE KEY,
 emp_no varchar(20) NOT NULL UNIQUE KEY,
 company_cd varchar(20) NOT NULL,
 company_nm varchar(100),
 pwd_enc varchar(200),
 first_name varchar(100),
 last_name varchar(100),
 name varchar(100),
 name_en varchar(100),
 email varchar(256),
 emp_type varchar(30),
 dept_cd varchar(20),
 dept_nm varchar(100),
 dept_sht_cd varchar(20),
 end_dept_cd varchar(20),
 end_dept_nm varchar(100),
 end_dept_sht_cd varchar(20),
 dept_grd_nm varchar(100),
 job_grp_cd varchar(20),
 job_grp_nm varchar(100),
 duty_cd varchar(20),
 duty_nm varchar(100),
 pos_cd varchar(20),
 pos_nm varchar(100),
 grd_cd varchar(20),
 grd_nm varchar(100),
 job_cd varchar(20),
 job_nm varchar(100),
 appn_cd varchar(20),
 appn_grd_cd varchar(20),
 appn_nm varchar(100),
 loc_cd varchar(20),
 loc_nm varchar(100),
 corp_cd varchar(10),
 start_ymd varchar(20),
 retire_ymd varchar(20),
 retire_yn varchar(1),
 rest_yn varchar(1),
 mas_ymd varchar(20),
 officephone varchar(50),
 cellphone varchar(50),
 kostl varchar(30),
 otp_yn varchar(1),
 ledger_kind varchar(20),
 sso_grd_cd varchar(20),
 ac_use_yn varchar(1),
 ac_reg_dtt date,
ac_chg_dttm date);

CREATE TABLE IF NOT EXISTS sys_shop_info(
shop_cd varchar(20) not null,
primary key(shop_cd),
shop_tcd varchar(2) not null,
fact_cd varchar(20) not null,
CONSTRAINT fk_sysshopinfo_syscomcd_factcd FOREIGN KEY(fact_cd) REFERENCES sys_com_cd(com_cd_id),
lang_cd varchar(3) not null,
sys_nm varchar(40),
sys_desc_sum varchar(200),
sys_desc text,
use_yn char(1),
sys_admin_id varchar(20),
affr_crgr_id varchar(20),
org_cd varchar(10),
reg_dt date,
reg_user_id varchar(20) NOT NULL,
CONSTRAINT fk_sysshopinfo_eluserinfo_reguserid FOREIGN KEY(reg_user_id) REFERENCES el_user_info(user_id),
modi_user_id varchar(20),
CONSTRAINT fk_sysshopinfo_eluserinfo_modiuserid FOREIGN KEY(modi_user_id) REFERENCES el_user_info(user_id),
modi_dt date,
reg_user_nm varchar(20),
modi_user_nm varchar(20),
sort_no int,
vehl_dsp_cd char(1),
shift_cd varchar(20),
CONSTRAINT fk_sys_shop_info_syscomcd_shif_cd FOREIGN KEY(shift_cd) REFERENCES sys_com_cd(com_cd_id));

CREATE TABLE IF NOT EXISTS bdm_comp(
comp_cd varchar(32) NOT NULL unique key,
comp_nm_kr varchar(1024) NOT NULL,
comp_nm_en varchar(128) NOT NULL,
ctgr_cd varchar(2),
reg_dttm datetime,
reg_user_id nvarchar(40),
modi_dttm datetime,
modi_user_id nvarchar(4),
PRIMARY KEY(comp_cd));

CREATE TABLE IF NOT EXISTS el_dept_info(
gubun varchar(20),
dept_cd varchar(100) NOT NULL,
