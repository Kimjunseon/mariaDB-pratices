  CONSTRAINT `fk_rominspresmixrm_bdmrominspmixrm_rominspmixrmid` FOREIGN KEY (`rom_insp_mixrm_id`) REFERENCES `bdm_rom_insp_mixrm` (`rom_insp_mixrm_id`),
  CONSTRAINT `fk_rominspresmixrm_bdmrominsptmset_insptmsetid` FOREIGN KEY (`insp_tmset_id`) REFERENCES `bdm_rom_insp_tmset` (`insp_tmset_id`),
  CONSTRAINT `fk_rominspresmixrm_eluserinfo_modiuserid` FOREIGN KEY (`modi_user_id`) REFERENCES `ei_user_info` (`user_id`),
  CONSTRAINT `fk_rominspresmixrm_eluserinfo_reguserid` FOREIGN KEY (`input_user_id`) REFERENCES `ei_user_info` (`user_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

