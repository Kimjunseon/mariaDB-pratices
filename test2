-- 테이블 sfrms.bdm_dpu_insp 구조 내보내기
CREATE TABLE IF NOT EXISTS `bdm_dpu_insp` (
  `insp_id` varchar(20) NOT NULL,
  `dpu_mst_id` varchar(20) NOT NULL,
  `manual_yn` char(1) DEFAULT NULL,
  `reg_dttm` date DEFAULT NULL,
  `reg_user_id` varchar(20) NOT NULL,
  `modi_user_id` varchar(20) DEFAULT NULL,
  `modi_dttm` date DEFAULT NULL,
  `shop_cd` varchar(20) NOT NULL,
  `shop_tcd` varchar(2) NOT NULL,
  PRIMARY KEY (`insp_id`),
  UNIQUE KEY `insp_id` (`insp_id`),
  KEY `fk_bdmdpuinsp_eluserinfo_reguserid` (`reg_user_id`),
  KEY `fk_bdmdpuinsp_eluserinfo_modiuserid` (`modi_user_id`),
  KEY `fk_bdmdpuinsp_sysshopinfo_shopcd` (`shop_cd`),
  CONSTRAINT `fk_bdmdpuinsp_eluserinfo_modiuserid` FOREIGN KEY (`modi_user_id`) REFERENCES `ei_user_info` (`user_id`),
  CONSTRAINT `fk_bdmdpuinsp_eluserinfo_reguserid` FOREIGN KEY (`reg_user_id`) REFERENCES `ei_user_info` (`user_id`),
  CONSTRAINT `fk_bdmdpuinsp_sysshopinfo_shopcd` FOREIGN KEY (`shop_cd`) REFERENCES `sys_shop_info` (`shop_cd`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

-- 테이블 데이터 sfrms.bdm_dpu_insp:~0 rows (대략적) 내보내기
