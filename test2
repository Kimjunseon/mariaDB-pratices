-- 테이블 sfrms.bdm_dpu_mnl 구조 내보내기
CREATE TABLE IF NOT EXISTS `bdm_dpu_mnl` (
  `dpu_mnl_id` varchar(20) NOT NULL,
  `dpu_insp_id` varchar(20) NOT NULL,
  `manual_nm` varchar(20) DEFAULT NULL,
  `manual_txt` text DEFAULT NULL,
  `manual_path` varchar(200) DEFAULT NULL,
  `reg_dttm` date DEFAULT NULL,
  `reg_user_id` varchar(20) NOT NULL,
