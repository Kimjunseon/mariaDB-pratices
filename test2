  `rom_proc_mgnt_id` varchar(20) NOT NULL,
  `shift_cd` varchar(20) NOT NULL,
  `type_cd` char(1) DEFAULT NULL,
  `img_yn` char(1) DEFAULT NULL,
  `detail_yn` char(1) DEFAULT NULL,
  `vehl_yn` char(1) DEFAULT NULL,
  `vehl_cnt` int(11) DEFAULT NULL,
  `equip_yn` char(1) DEFAULT NULL,
  `equip_cnt` int(11) DEFAULT NULL,
  `manual_yn` char(1) DEFAULT NULL,
  `empha_yn` char(1) DEFAULT NULL,
  `use_yn` char(1) DEFAULT NULL,
  `reg_dttm` date DEFAULT NULL,
  `reg_user_id` varchar(20) NOT NULL,
  `modi_user_id` varchar(20) DEFAULT NULL,
  `modi_dttm` date DEFAULT NULL,
  `shop_cd` varchar(20) NOT NULL,
  `shop_tcd` varchar(2) NOT NULL,
  PRIMARY KEY (`rom_insp_id`),
  KEY `fk_bdmrominsp_eluserinfo_reguserid` (`reg_user_id`),
  KEY `fk_bdmrominsp_eluserinfo_modiuserid` (`modi_user_id`),
  KEY `fk_bdmrominsp_sysshopinfo_shopcd` (`shop_cd`),
  CONSTRAINT `fk_bdmrominsp_eluserinfo_modiuserid` FOREIGN KEY (`modi_user_id`) REFERENCES `ei_user_info` (`user_id`),
  CONSTRAINT `fk_bdmrominsp_eluserinfo_reguserid` FOREIGN KEY (`reg_user_id`) REFERENCES `ei_user_info` (`user_id`),
  CONSTRAINT `fk_bdmrominsp_sysshopinfo_shopcd` FOREIGN KEY (`shop_cd`) REFERENCES `sys_shop_info` (`shop_cd`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

