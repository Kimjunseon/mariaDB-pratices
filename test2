-- 테이블 sfrms.rom_insp_res 구조 내보내기
CREATE TABLE IF NOT EXISTS `rom_insp_res` (
  `rom_inspect_id` int(11) NOT NULL,
  `insp_tmset_id` varchar(20) NOT NULL,
  `day_value` float DEFAULT NULL,
  `inspect_yn` char(1) DEFAULT NULL,
  `done_yn` char(1) DEFAULT NULL,
  `input_dttm` date DEFAULT NULL,
  `input_user_id` varchar(20) NOT NULL,
  `modi_user_id` varchar(20) DEFAULT NULL,
  `modi_dttm` date DEFAULT NULL,
  `vehl_nm` varchar(20) DEFAULT NULL,
  `img_pack_id` int(11) DEFAULT NULL,
  `detail` text DEFAULT NULL,
  `ok_yn` char(1) DEFAULT NULL,
  `shop_cd` varchar(20) NOT NULL,
  `shop_tcd` varchar(2) NOT NULL,
  `rom_res_id` int(11) NOT NULL,
  `date_dt` datetime DEFAULT NULL,
  PRIMARY KEY (`rom_res_id`),
  KEY `fk_rominspres_bdmrominsptmset_insptmsetid` (`insp_tmset_id`),
  KEY `fk_rominspres_eluserinfo_reguserid` (`input_user_id`),
  KEY `fk_rominspres_eluserinfo_modiuserid` (`modi_user_id`),
  KEY `fk_rominspres_sysfilepack_imgpackid` (`img_pack_id`),
  KEY `fk_rominspres_sysshopinfo_shopcd` (`shop_cd`),
  CONSTRAINT `fk_rominspres_bdmrominsptmset_insptmsetid` FOREIGN KEY (`insp_tmset_id`) REFERENCES `bdm_rom_insp_tmset` (`insp_tmset_id`),
  CONSTRAINT `fk_rominspres_eluserinfo_modiuserid` FOREIGN KEY (`modi_user_id`) REFERENCES `ei_user_info` (`user_id`),
  CONSTRAINT `fk_rominspres_eluserinfo_reguserid` FOREIGN KEY (`input_user_id`) REFERENCES `ei_user_info` (`user_id`),
  CONSTRAINT `fk_rominspres_sysfilepack_imgpackid` FOREIGN KEY (`img_pack_id`) REFERENCES `sys_file_pack` (`file_pack_id`),
  CONSTRAINT `fk_rominspres_sysshopinfo_shopcd` FOREIGN KEY (`shop_cd`) REFERENCES `sys_shop_info` (`shop_cd`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

