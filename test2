CREATE TABLE IF NOT EXISTS `sys_menu_bas` (
  `menu_cd` varchar(20) NOT NULL,
  `lang_cd` varchar(3) NOT NULL,
  `menu_nm` varchar(100) NOT NULL,
  `level_no` int(11) DEFAULT NULL,
  `sort_sn` int(11) DEFAULT NULL,
  `screen_menu_yn` char(1) DEFAULT NULL,
  `use_yn` char(1) DEFAULT NULL,
  `supi_menu_cd` varchar(12) DEFAULT NULL,
  `pgm_id` varchar(10) DEFAULT NULL,
  `reg_user_id` varchar(20) NOT NULL,
  `reg_dt` date DEFAULT NULL,
  `modi_user_id` varchar(20) DEFAULT NULL,
  `modi_dt` date DEFAULT NULL,
  `shop_tcd` varchar(2) NOT NULL,
  PRIMARY KEY (`menu_cd`),
