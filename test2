-- 테이블 sfrms.bdm_comp 구조 내보내기
CREATE TABLE IF NOT EXISTS `bdm_comp` (
  `comp_cd` varchar(32) NOT NULL,
  `comp_nm_kr` varchar(1024) NOT NULL,
  `comp_nm_en` varchar(128) NOT NULL,
  `ctgr_cd` varchar(2) DEFAULT NULL,
  `reg_dttm` datetime DEFAULT NULL,
  `reg_user_id` varchar(40) DEFAULT NULL,
  `modi_dttm` datetime DEFAULT NULL,
  `modi_user_id` varchar(4) DEFAULT NULL,
  PRIMARY KEY (`comp_cd`),
  UNIQUE KEY `comp_cd` (`comp_cd`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

-- 테이블 데이터 sfrms.bdm_comp:~0 rows (대략적) 내보내기
